
*{
   margin:0;
   padding:0;
   font-family: Arial;
}

body {
  width: 100%;
  height: 100%;
  background: #e9e9e9;
  text-align:center;
}


img{
  display: inline-block;
  float: left;
  margin-top: 15px;
  margin-left: 20px;
  margin-right: 15px;
}

#main{
  width: 55%;
  height: 230px;
  margin:0 auto;
  margin-top: 30px;
  background: white;
  text-align:left;
}

.title {
 font-weight: bold;
 font-size: 35px;
 display: inline-block;
 margin-bottom: 10px;
}

.subtitle {
  font-size: 17px;
}

.subtitle2 {
  font-size: 20px;
  font-weight: normal;
  margin: 2px;
}
.subtitle3 {
  font-size: 25px;
  font-weight: normal;
  margin: 2px;

}


.normal {
 font-size: 12px;
 float:left;
 display: inline-block;
 margin-bottom: 5px;
}

.normalgrey {
  color: grey;
  font-size: 12px;
  margin-right: 15px;
   margin-bottom: 5px;

}

#studies{
  width: 55%;
  height: 230px;
  margin:0 auto;
  margin-top: 15px;
  background: white;
  text-align:left;
}

#tableStudies{
  margin-left: 20px;
   width: 100%;
}